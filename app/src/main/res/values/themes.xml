<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="Theme.noActionBar" parent="Theme.AppCompat.DayNight.NoActionBar"></style>

    <style name="TextView"/>

    <style name="TextView.PoppinsMedium32spblack">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">32sp</item>
    </style>

    <style name="TextView.PoppinsMedium18spblack">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="TextView.PoppinsNormal16spblack">
        <item name="android:fontFamily">@font/poppins_thin</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="EditText"></style>

    <style name="EditText.PoppinsNormal14spabu">
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textColor">@color/abu</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Button"></style>

    <style name="Button.Primary">
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_orange</item>
    </style>

    <style name="Button.Secondary">
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_orange_muda</item>
    </style>
</resources>